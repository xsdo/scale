    </sql>

    <select id="selectScaleTaskLists" parameterType="ScaleTask" resultMap="ScaleTaskResult">
        <include refid="selectScaleTaskVos"/>
        b.scale_name as scaleName
        from scale_task a
        left join scale_menu b
        on a.scale_id = b.id
        <where>
            a.task_status!='3'
            <if test="userId != null "> and a.user_id = #{userId}</if>
            <if test="userName != null  and userName != ''"> and a.user_name like concat('%', #{userName}, '%')</if>
            <if test="patientId != null "> and a.patient_id = #{patientId}</if>
            <if test="patientName != null  and patientName != ''"> and a.patient_name like concat('%', #{patientName}, '%')</if>

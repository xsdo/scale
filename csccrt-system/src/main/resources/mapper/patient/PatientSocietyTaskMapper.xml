    <sql id="selectPatientSocietyTaskVo">
        select task_id, user_id, user_name, patient_id, patient_name, workstation, test_coding, typeIds, task_status, del_flag, create_by, create_time, update_by, update_time,scale_id from patient_society_task
    </sql>

    <select id="selectPatientSocietyTaskList" parameterType="PatientSocietyTask" resultMap="PatientSocietyTaskResult">
        <include refid="selectPatientSocietyTaskVo"/>
        <where>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="userName != null  and userName != ''"> and user_name like concat('%', #{userName}, '%')</if>
            <if test="patientId != null "> and patient_id = #{patientId}</if>
            <if test="patientName != null  and patientName != ''"> and patient_name like concat('%', #{patientName}, '%')</if>
            <if test="workstation != null  and workstation != ''"> and workstation = #{workstation}</if>
            <if test="testCoding != null  and testCoding != ''"> and test_coding = #{testCoding}</if>
            <if test="typeIds != null  and typeIds != ''"> and typeIds = #{typeIds}</if>
            <if test="taskStatus != null  and taskStatus != ''"> and task_status = #{taskStatus}</if>
            <if test="delFlag != null  and delFlag != ''"> and del_flag = #{delFlag}</if>
            <if test="createBy != null  and createBy != ''">and create_by = #{createBy}</if>
            <if test="createTime != null  and createTime != ''">and create_time = #{createTime}</if>
        </where>
        order by create_time desc
    </select>

    <select id="selectPatientSocietyTaskById" parameterType="Long" resultMap="PatientSocietyTaskResult">
        <include refid="selectPatientSocietyTaskVo"/>
        where task_id = #{taskId}
    </select>
    <select id="selectPatientTaskByPatientId"  resultMap="PatientSocietyTaskResult">
        <include refid="selectPatientSocietyTaskVo"/>
        where patient_id = #{patientId} AND del_flag=#{delFlag}
    </select>
    <select id="selectAllByPatientId" parameterType="Long" resultMap="PatientSocietyTaskResult">
        <include refid="selectPatientSocietyTaskVo"/>
        where patient_id = #{patientId}
    </select>
    <insert id="insertPatientSocietyTask" parameterType="PatientSocietyTask" useGeneratedKeys="true" keyProperty="taskId">
        insert into patient_society_task
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null ">user_id,</if>
            <if test="userName != null  and userName != ''">user_name,</if>
            <if test="patientId != null ">patient_id,</if>
            <if test="patientName != null  and patientName != ''">patient_name,</if>
            <if test="workstation != null  and workstation != ''">workstation,</if>
            <if test="testCoding != null  and testCoding != ''">test_coding,</if>
            <if test="typeIds != null  and typeIds != ''">typeIds,</if>
            <if test="taskStatus != null  and taskStatus != ''">task_status,</if>
            <if test="delFlag != null  and delFlag != ''">del_flag,</if>
            <if test="createBy != null  and createBy != ''">create_by,</if>
            <if test="createTime != null ">create_time,</if>
            <if test="updateBy != null  and updateBy != ''">update_by,</if>
            <if test="updateTime != null ">update_time,</if>
            <if test="scaleId != null ">scale_id,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null ">#{userId},</if>
            <if test="userName != null  and userName != ''">#{userName},</if>
            <if test="patientId != null ">#{patientId},</if>
            <if test="patientName != null  and patientName != ''">#{patientName},</if>

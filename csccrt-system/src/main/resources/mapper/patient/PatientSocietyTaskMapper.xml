    </update>
    <delete id="deletePatientSocietyTaskById" parameterType="Long">
        delete from patient_society_task where task_id = #{taskId}
    </delete>

    <delete id="deletePatientSocietyTaskByIds" parameterType="String">
        delete from patient_society_task where task_id in
        <foreach item="taskId" collection="array" open="(" separator="," close=")">
            #{taskId}
        </foreach>
    </delete>
    <select id="selectTaskByWorkStation" parameterType="String" resultMap="PatientSocietyTaskResult">
        <include refid="selectPatientSocietyTaskVo"/>
        where workstation = #{workStation} and task_status!=3
